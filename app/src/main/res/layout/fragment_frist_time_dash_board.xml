<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
   >
    <data>
        <variable
            name="FristtimeDashboardViewModel"
            type="com.wecompli.viewmodel.FristDashBoardViewModel" />
    </data>
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/top_two_rounded_corner_white">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_250sdp"
        android:layout_centerInParent="true"
        android:background="@drawable/rectangular_shape_rounded_corner_white"
        android:layout_margin="@dimen/_18sdp"
        android:orientation="vertical">
            <LinearLayout
                android:id="@+id/ll_adduser"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_70sdp"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_marginRight="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_35sdp"
                android:onClick="@{FristtimeDashboardViewModel.openAddSite}"
                android:background="@drawable/rectangular_shape_rounded_corner_light_blue">
                <RelativeLayout
                    android:layout_width="@dimen/_55sdp"
                    android:layout_height="@dimen/_55sdp"
                    android:layout_marginLeft="@dimen/_15sdp"
                    android:background="@drawable/circle_blue"
                    android:layout_gravity="center_vertical">
                    <ImageView
                        android:layout_width="@dimen/_33sdp"
                        android:layout_height="@dimen/_35sdp"
                        android:layout_centerInParent="true"
                        android:background="@drawable/add_site">

                    </ImageView>
                </RelativeLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="@dimen/_20sdp"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_16sdp"
                    app:font="@{`rajDhaniBold`}"
                    android:text="@string/add_user"></TextView>
            </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_addsite"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_70sdp"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_25sdp"
            android:layout_marginTop="@dimen/_35sdp"
            android:onClick="@{FristtimeDashboardViewModel.openAddUser}"
            android:background="@drawable/botton_rounded_corner">
            <RelativeLayout
                android:layout_width="@dimen/_55sdp"
                android:layout_height="@dimen/_55sdp"
                android:layout_marginLeft="@dimen/_15sdp"
                android:background="@drawable/circle_sky_blue"
                android:layout_gravity="center_vertical">
                <ImageView
                    android:layout_width="@dimen/_33sdp"
                    android:layout_height="@dimen/_35sdp"
                    android:layout_centerInParent="true"
                    android:background="@drawable/add_user">

                </ImageView>
            </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_marginLeft="@dimen/_20sdp"
                android:textColor="@color/white"
                android:textSize="@dimen/_16sdp"
                app:font="@{`rajDhaniBold`}"
                android:text="@string/addsite"></TextView>
        </LinearLayout>

    </LinearLayout>

</RelativeLayout>


</layout>